<T3DataStructure>
    <meta>
       <langDisable>1</langDisable> 
	  <!--  <langChildren>1</langChildren>!-->
        </meta>
  <!--<sheets>
	<sDEF>-->
	  <ROOT>
	    <type>array</type>
	    <el>
	      <mode>
			  <TCEforms>
				<label>Mode</label>
				<config>
					<type>radio</type>
					<items>
						<n0>
							<n0>Query defined in FE</n0>
							<n1>0</n1>
						</n0>
						<n1>
							<n0>Query defined in plugin</n0>
							<n1>1</n1>
						</n1>
					</items>
				</config>
			  </TCEforms>
		  </mode>	
		  <owner>
			  <TCEforms>
				<label>Owner</label>
				<config>
					<type>select</type>
					<items>
						<n0>
							<n0>ALL</n0>
							<n1>0</n1>
						</n0>
					</items>
					<foreign_table>tx_docdb_owner</foreign_table>
					<minitems>0</minitems>
					<maxitems>1</maxitems>
					<size>1</size>
				</config>
			  </TCEforms>
		  </owner>	
		  <docType>
			  <TCEforms>
				<label>Document Type</label>
				<config>
					<type>select</type>
					<items>
						<numIndex index="0" type="array">
							<numIndex index="0">ALL</numIndex>
							<numIndex index="1">0</numIndex>
						</numIndex>
					</items>
					<foreign_table>tx_docdb_type</foreign_table>
					<minitems>0</minitems>
					<maxitems>1</maxitems>
					<size>1</size>
				</config>
			  </TCEforms>
		  </docType>
		  <docStatus>
			  <TCEforms>
				<label>Document Status</label>
				<config>
					<type>select</type>
					<items>
						<numIndex index="0" type="array">
							<numIndex index="0">ALL</numIndex>
							<numIndex index="1">0</numIndex>
						</numIndex>
					</items>
					<foreign_table>tx_docdb_status</foreign_table>
					<minitems>0</minitems>
					<maxitems>1</maxitems>
					<size>1</size>
				</config>
			  </TCEforms>
		  </docStatus>
		  <descr_combination>
			  <TCEforms>
				<label>Descriptors combination</label>
				<config>
					<type>radio</type>
					<items>
						<numIndex index="0" type="array">
							<numIndex index="0">AND</numIndex>
							<numIndex index="1">0</numIndex>
						</numIndex>
						<numIndex index="1" type="array">
							<numIndex index="0">OR</numIndex>
							<numIndex index="1">1</numIndex>
						</numIndex>
						<!--<n0>
							<n0>AND</n0>
							<n1>0</n1>
						</n0>
						<n1>
							<n0>OR</n0>
							<n1>1</n1>
						</n1>-->
					</items>
				</config>
			  </TCEforms>
		  </descr_combination>	
		  <descriptors>
		  	<TCEforms>
			<exclude>1</exclude>
			<label>Descriptors</label>
			<config>
				<type>select</type>
				<!--<foreign_field>dscr_pid</foreign_field>-->
				<foreign_table>tx_docdb_descriptor</foreign_table>
				<foreign_field>uid</foreign_field>
				<foreign_selector>dscr_pid</foreign_selector>
			   	<foreign_unique>dscr_pid</foreign_unique>
				<internal_type>db</internal_type>
				<!--<allowed>tx_docdb_descriptor</allowed>-->
				<form_type>user</form_type>
				<userFunc>tx_docdb_treeview->displayCategoryTree</userFunc>
				<treeView>1</treeView>
				<size>3</size>
				<autoSizeMax>30</autoSizeMax>
				<minitems>0</minitems>
				<maxitems>500</maxitems>
  				<!--<MM>pages_tx_docdb_doc_descriptor_mm</MM> 
				<type>group</type>
				<internal_type>db</internal_type>
				<allowed>tx_docdb_descriptor</allowed>
				<size>3</size>
				<maxitems>22</maxitems>
				<minitems>0</minitems>
				<show_thumbs>0</show_thumbs>-->
			</config>
		  	</TCEforms>
		  </descriptors>
		  <template_file>
			<TCEforms>
				<label>Template file</label>
				<config>
					<type>input</type>
		 			<eval>trim</eval>
 				<size>28</size>
 				<wizards type="array">
  					<link type="array">
   						<type>popup</type>
   						<title>Template file</title>
   						<icon>link_popup.gif</icon>
   						<script>browse_links.php?act=file&amp;mode=wizard</script>
   						<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
  					</link>
 				</wizards>
				</config>
			</TCEforms>
		</template_file>
            <sorting>
		  <TCEforms>
				<label>Default sorting</label>
				<config>
					<type>radio</type>
					<items>
						<n0>
							<n0>By title</n0>
							<n1>title</n1>
						</n0>
						<n1>
							<n0>By date updated</n0>
							<n1>lastUpdated</n1>
						</n1>
						<n2>
						      <n0>By KeyRef</n0>
						      <n1>tx_docdb_doc_key</n1>
						</n2>
					</items>
				</config>
			  </TCEforms>
		  </sorting>
              <sorting_order>
		  <TCEforms>
				<label>Sorting order</label>
				<config>
					<type>radio</type>
					<items>
						<n0>
							<n0>Ascending</n0>
							<n1>ASC</n1>
						</n0>
						<n1>
							<n0>Descending</n0>
							<n1>DESC</n1>
						</n1>
					</items>
				</config>
			  </TCEforms>
		  </sorting_order>	

		  <!--<addWhere>
			  <TCEforms>
				<label>Where Clause</label>
				<config>
					<type>text</type>
					<minitems>0</minitems>
					<maxitems>1</maxitems>
					<rows>3</rows>
					<size>30</size>
				</config>
			  </TCEforms>
		  </addWhere>-->
	    </el>
	  </ROOT>
